{
  "headers": [
    {
      "source": "/(.*).(jpg|jpeg|png|gif|webp|ico|svg|avif)",
      "headers": [{ "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }]
    },
    {
      "source": "/(.*).(css|js)",
      "headers": [{ "key": "Cache-Control", "value": "public, max-age=2592000, immutable" }]
    }
  ]
}
